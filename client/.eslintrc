{
  "extends": ["react-app", "react-app/jest"],
  "rules": {
    "prefer-const": "error",
    "quotes": ["warn", "double"],
    "jsx-quotes": ["warn", "prefer-double"],
    "semi": ["warn", "always"],
    "import/order": [
      "error",
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index",
          "object",
          "type"
        ],
        "newlines-between": "always"
      }
    ]
  }
}